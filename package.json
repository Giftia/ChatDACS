  "scripts": {
    "run": "node index.js",
    "pkg": "pkg .",
    "docker": "docker build . -t giftina/chatdacs"
  },
  "dependencies": {
    "axios": "0.27.2",
    "bilibili-live-ws": "6.2.1",
    "canvas": "2.9.3",
    "colors": "1.4.0",
    "compression": "1.7.4",
    "cookie": "0.5.0",
    "diff": "5.1.0",
    "express": "4.18.1",
    "lib-qqwry": "1.3.2",
    "multer": "1.4.4",
    "music-metadata": "7.12.4",
    "nodejieba": "2.5.2",
    "play-sound": "1.1.5",
    "qq-guild-bot": "2.9.5",
    "request": "2.88.2",
    "require.all": "2.0.5",
    "socket.io": "4.5.1",
    "sqlite3": "5.0.10",
    "winston": "3.8.1",
    "yaml": "2.1.1"
  },
  "devDependencies": {
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "mp3-duration": "1.1.0",
    "rss-parser": "3.12.0",
    "trayicon": "1.2.2"
  },
  "repository": {
    "type": "git",
    "url": "//github.com/Giftia/ChatDACS.git"
  },
  "bin": "index.js",
  "pkg": {
    "assets": [
      "node_modules/canvas/build/Release/*",
      "node_modules/sqlite3/lib/binding/napi-v6-win32-unknown-x64/node_sqlite3.node",
      "node_modules/sqlite3/lib/binding/napi-v6-linux-glibc-x64/node_sqlite3.node",
      "node_modules/nodejieba/build/Release/nodejieba.node",
      "node_modules/socket.io/client-dist/*"
    ],
    "targets": [
      "node14-win-x64"
    ],
    "outputPath": "."
  },
  "engines": {
    "node": "~14"
  }
}
