{
  "name": "chatdacs",
  "version": "3.8.0",
  "decription": "Chatbot : shaDiao Ai Chat System",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "node index.js",
    "pkg": "pkg .",
    "docker-build": "docker build . -t giftina/chatdacs:",
    "migrate-generate": "npx sequelize-cli migration:generate --name",
    "migrate-dev": "npx sequelize-cli db:migrate",
    "migrate": "npx sequelize-cli db:migrate --env production",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --env production"
  },
  "dependencies": {
    "axios": "0.28.1",
    "bilibili-live-ws": "6.3.1",
    "colors": "1.4.0",
    "compression": "1.7.4",
    "cookie": "0.6.0",
    "dayjs": "1.11.13",
    "diff": "6.0.0",
    "express": "4.19.2",
    "jimp": "0.22.12",
    "lib-qqwry": "1.3.4",
    "mp3-duration": "1.1.0",
    "multer": "1.4.4",
    "node-schedule": "2.1.1",
    "node-telegram-bot-api": "0.66.0",
    "nodejs-jieba": "0.1.2",
    "play-sound": "1.1.6",
    "qq-guild-bot": "2.9.5",
    "request": "2.88.2",
    "require.all": "2.0.5",
    "rss-parser": "3.13.0",
    "semver-diff": "3.1.1",
    "sequelize": "6.37.3",
    "socket.io": "4.7.5",
    "sqlite3": "5.1.7",
    "trayicon": "1.2.2",
    "winston": "3.14.2",
    "yaml": "2.5.0"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-node": "11.1.0",
    "sequelize-cli": "^6.5.2"
  },
  "repository": {
    "type": "git",
    "url": "//github.com/Giftia/ChatDACS.git"
  },
  "bin": "index.js",
  "pkg": {
    "assets": [
      "node_modules/canvas/build/Release/*",
      "node_modules/sqlite3/lib/binding/napi-v6-win32-unknown-x64/node_sqlite3.node",
      "node_modules/sqlite3/lib/binding/napi-v6-linux-glibc-x64/node_sqlite3.node",
      "node_modules/socket.io/client-dist/*"
    ],
    "targets": [
      "node14-win-x64"
    ],
    "outputPath": "."
  },
  "engines": {
    "node": "~18"
  }
}