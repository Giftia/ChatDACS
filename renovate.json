{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true,
      "automergeType": "branch",
      "matchDatasources": [
        "npm"
      ],
      "stabilityDays": 3,
      "groupName": "dependencies"
    }
  ],
  "baseBranches": [
    "dev"
  ]
}